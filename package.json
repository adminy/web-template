{
	"name": "magic_stream",
	"version": "1.0.0",
	"description": "Magic Stream Server, Web & App",
	"scripts": {
		"frontend": "parcel watch ./web/index.html",
		"backend": "nodemon server/index.js",
		"start": "concurrently --kill-others \"npm run frontend\" \"npm run backend\"",
		"prod": "parcel build ./web/index.html && node server/index.js"
	},
	"dependencies": {
		"fastify": "^3.9.1",
		"fastify-https-redirect": "^1.0.3",
		"fastify-static": "^3.3.0",
		"fastify-websocket": "^2.0.11",
		"jsx-dom": "^7.0.0-beta.2",
		"middie": "^5.2.0"
	},
	"devDependencies": {
		"choo": "^7.1.0",
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.6",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.30.0"
	},
	"author": "Marin Bivol",
	"license": "ISC"
}
